{
    "name": "bassta/template",
    "description": "Simple template",
    "type": "project",
    "authors": [
        {
            "name": "Vasily Stashko",
            "email": "vasily.stashko@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "rector/rector": "^0.13.7",
        "symplify/easy-coding-standard": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests",
            "Utils\\PHPStan\\": "utils/PHPStan/src"
        }
    }
}
